{
    "bitwise": false,
    "camelcase": true,
    "curly": false,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": false,
    "noarg": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": false,
    "regexp": false,
    "undef": true,
    "unused": "vars",
    "strict": true,
    "loopfunc": true,
    "trailing": true,
    "node": true,
    "noempty": true,
    "maxdepth": 4,
    "maxparams": 5,
    "globals": {
        "console": true,
        "Buffer": true,
        "setTimeout": true,
        "clearTimeout": true,
        "setInterval": true,
        "clearInterval": true
    }
}
