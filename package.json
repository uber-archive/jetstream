{
  "name": "jetstream",
  "version": "0.2.1",
  "description": "Jetstream Sync server framework to sync local and remote models",
  "keywords": [
    "framework",
    "realtime",
    "modular"
  ],
  "author": "Rob Skillington <r@uber.com>",
  "repository": "git://github.com/uber/jetstream.git",
  "main": "index",
  "homepage": "https://github.com/uber/jetstream",
  "contributors": [
    {
      "name": "Rob Skillington"
    },
    {
      "name": "Tuomas Artman"
    }
  ],
  "scripts": {
    "jshint": "jshint --verbose .",
    "test": "tape test/index.js | tap-spec",
    "test-cover": "istanbul cover tape -- test/index.js | tap-spec",
    "view-cover": "istanbul report html && open ./coverage/index.html",
    "test-coveralls": "istanbul cover tape --report lcovonly -- test/index.js | tap-spec && cat ./coverage/lcov.info | coveralls",
    "start": "node demos/app.js",
    "check-licence": "uber-licence --dry",
    "add-licence": "uber-licence"
  },
  "pre-commit": [
    "jshint",
    "test",
    "check-licence"
  ],
  "dependencies": {
    "async": "^0.9.0",
    "body-parser": "^1.6.5",
    "callback-or-emit-error": "^1.0.1",
    "debug": "^1.0.4",
    "deepequal": "0.0.1",
    "dotty": "0.0.2",
    "error": "^4.3.0",
    "express": "^4.8.5",
    "lodash": "^2.4.1",
    "maybe-callback": "^1.0.0",
    "node-uuid": "^1.4.1",
    "replr": "^0.2.4",
    "request": "^2.51.0",
    "request-debug": "^0.1.1",
    "robb": "^0.2.1",
    "rust-result": "^0.1.0",
    "semver": "^4.0.0",
    "tryit": "^1.0.1",
    "winston": "^0.7.3",
    "ws": "^0.4.32"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "coveralls": "^2.11.2",
    "grunt": "^0.4.5",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-run": "^0.2.4",
    "istanbul": "^0.3.0",
    "jshint": "^2.5.11",
    "mocha": "^1.21.4",
    "nodemon": "^1.2.1",
    "open": "0.0.5",
    "opn": "^1.0.0",
    "pre-commit": "0.0.9",
    "redtape": "^1.0.0",
    "sinon": "^1.10.3",
    "tap-spec": "^1.0.0",
    "tape": "^3.0.3",
    "uber-licence": "^1.1.0",
    "underscore": "^1.6.0"
  },
  "bugs": {
    "url": "https://github.com/uber/jetstream/issues",
    "email": "r@uber.com"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/uber/jetstream/LICENSE"
    }
  ]
}
